set showmatch
set tags=tags,../tags,/usr/local/tags,/usr/include/tags
let Tlist_WinWidth=32
"set autochdir
syntax on
set hlsearch
"set nu
set fileencodings=utf-8,gbk
set ts=4
set nocompatible
"set backspace=2
filetype plugin indent on
set shiftwidth=4
set expandtab
let g:org_export_emacs="/usr/bin/emacs"
let g:org_export_verbose=1

"nmap <leader>md :%!/usr/local/bin/Markdown.pl --html4tags <cr>
set paste
